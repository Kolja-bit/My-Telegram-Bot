<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog-1.0.xsd">
    <changeSet id="createNewsTable" author="root">
        <createTable tableName="subscribers">
            <column name="id" type="serial">
                <constraints primaryKey="true"/>
            </column>
            <column name="uuidUser" type="uuid"/>
            <column name="telegramUserID" type="long"/>
            <column name="userSubscriptionPrice" type="double"/>
        </createTable>
    </changeSet>

</databaseChangeLog>